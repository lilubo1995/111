#pragma once
class myteacher
{
private:
	int m_age;
	
	char m_name[32];
public:
	void setage(int age);
	int getage();
};

#include "myteacher.h"
void myteacher::setage(int age)
{
	m_age = age;
}
int myteacher::getage()
{
	return m_age;
}


#include<iostream>
using namespace std;
#include "myteacher.h"
int main() {
	myteacher t1;
	t1.setage(36);
	cout << t1.getage() << endl;
	system("pause");
	return 0;
}



